<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - MusicLab.Frontend</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/font-awesome/css/all.min.css" />
    <link rel="stylesheet" href="~/MusicLab.Frontend.styles.css" asp-append-version="true" />
</head>
<body>
        
    @RenderBody()

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/TooltipBootstrap.js" asp-append-version="true"></script>
    <script src="~/js/callAPI.js" asp-append-version="true"></script>
    <script src="~/js/SearchKeywordPlaylist.js" asp-append-version="true"></script>
    <script src="~/js/SongArtist.js" asp-append-version="true"></script>
    <script src="~/js/PlayMusic.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>